{
  "name": "me-io/php-ini-parser",
  "description": "A Zend_Config_Ini like parser for .ini files.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "configuration",
    "ini"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6",
    "fzaninotto/faker": "^1",
    "squizlabs/php_codesniffer": "^3",
    "overtrue/phplint": "^1"
  },
  "autoload": {
    "psr-4": {
      "Ini\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true
  }
}
